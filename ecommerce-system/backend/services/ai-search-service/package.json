{
  "name": "ai-search-service",
  "version": "1.0.0",
  "description": "AI-powered search service for e-commerce platform",
  "main": "src/app.py",
  "scripts": {
    "start": "python -m uvicorn src.app:app --host 0.0.0.0 --port 3014",
    "dev": "python -m uvicorn src.app:app --host 0.0.0.0 --port 3014 --reload",
    "start-script": "./start.sh",
    "test": "pytest tests/",
    "lint": "flake8 src/",
    "format": "black src/ tests/"
  },
  "keywords": [
    "ai",
    "search",
    "vector",
    "milvus",
    "ecommerce"
  ],
  "author": "E-commerce Team",
  "license": "MIT",
  "dependencies": {
    "flask": "^3.0.0",
    "flask-cors": "^4.0.0",
    "flask-restx": "^1.3.0",
    "pymilvus": "^2.3.0",
    "numpy": "^1.24.0",
    "sentence-transformers": "^2.2.2",
    "redis": "^5.0.0",
    "requests": "^2.31.0",
    "python-dotenv": "^1.0.0",
    "gunicorn": "^21.2.0",
    "pydantic": "^2.5.0",
    "loguru": "^0.7.2"
  },
  "devDependencies": {
    "pytest": "^7.4.0",
    "pytest-flask": "^1.3.0",
    "pytest-cov": "^4.1.0",
    "black": "^23.0.0",
    "flake8": "^6.0.0",
    "mypy": "^1.7.0"
  },
  "engines": {
    "python": ">=3.9"
  }
}
